{
	"author": "Ólavur Nón",
	"name": "oauth-spa-js",
	"description": "Single-Page Application Authorization Client for the OAuth 2.0 PKCE flow",
	"license": "MIT",
	"version": "0.8.0",
	"homepage": "https://github.com/OTheNonE/oauth-spa-js",
	"repository": {
		"type": "git",
		"url": "https://github.com/OTheNonE/oauth-spa-js"
	},
	"bugs": "https://github.com/OTheNonE/oauth-spa-js/issues",
	"keywords": [
		"Single Page Application",
		"Authorization",
		"SDK",
		"OAuth",
		"OpenID Connect",
		"PKCE"
	],
	"type": "module",
	"main": "dist/cjs/index.js",
	"types": "dist/cjs/index.d.ts",
	"module": "dist/esm/index.js",
	"exports": {
		"require": "./dist/cjs/index.js",
		"import": "./dist/esm/index.js"
	},
	"scripts": {
		"dev": "vite dev",
		"host": "vite dev --host",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "test",
		"build": "npm run build:cjs && npm run build:esm",
		"build:cjs": "tsc -p tsconfig.cjs.json",
		"build:esm": "tsc -p tsconfig.esm.json"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"publint": "^0.1.9",
		"svelte": "^5.0.0-next.210",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.11"
	}
}
